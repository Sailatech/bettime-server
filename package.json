{
  "name": "pdf-manager-backend",
  "version": "1.0.0",
  "description": "PDF Manager and Library System",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.882.0",
    "@aws-sdk/s3-request-presigner": "^3.882.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.8.4",
    "bcryptjs": "^2.4.3",
    "canvas": "^3.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.0.1",
    "helmet": "^6.2.0",
    "jimp": "^1.6.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.0",
    "node-fetch": "^3.3.2",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.17.0",
    "qrcode": "^1.5.4",
    "sanitize-filename": "^1.6.3",
    "sharp": "^0.32.0",
    "uuid": "^9.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^3.1.9",
    "prettier": "^2.8.7",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/pdf-manager.git"
  },
  "keywords": [
    "pdf",
    "manager",
    "library",
    "express",
    "mysql",
    "node"
  ],
  "author": "Nairasmine",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/pdf-manager/issues"
  },
  "homepage": "https://github.com/yourusername/pdf-manager#readme",
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage",
    "collectCoverage": true
  },
  "nodemonConfig": {
    "ignore": [
      "uploads/*",
      "tests/*",
      "coverage/*"
    ],
    "delay": "2500"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ]
  }
}
